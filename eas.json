{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "hooks": {
        "preInstall": ".eas-hooks/eas-build-pre-install.sh"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      },
      "hooks": {
        "preInstall": ".eas-hooks/eas-build-pre-install.sh"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "hooks": {
        "preInstall": ".eas-hooks/eas-build-pre-install.sh"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
