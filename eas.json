{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "development"
      },
      "prebuildCommand": "echo 'export const METRO_CONFIG_PATH = \"./metro.config.js\";' > metro-config-path.js"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "env": {
        "APP_ENV": "staging"
      },
      "prebuildCommand": "echo 'export const METRO_CONFIG_PATH = \"./metro.config.js\";' > metro-config-path.js"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "APP_ENV": "production"
      },
      "prebuildCommand": "echo 'export const METRO_CONFIG_PATH = \"./metro.config.js\";' > metro-config-path.js"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "ozan05avci@gmail.com",
        "ascAppId": "6745790562",
        "appleTeamId": "9RFBD6KWF9"
      }
    }
  }
}
