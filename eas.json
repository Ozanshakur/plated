{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "node": "16.20.0",
      "env": {
        "EXPO_NO_DOTENV": "1",
        "CI": "1",
        "LEGACY_PEER_DEPS": "1"
      },
      "ios": {
        "resourceClass": "m-medium",
        "image": "macos-monterey-12.6-xcode-14.2",
        "buildConfiguration": "Release",
        "credentialsSource": "remote",
        "distribution": "store",
        "simulator": false,
        "prebuildCommand": "node expo-prebuild.js",
        "env": {
          "EXPO_USE_HERMES": "0"
        }
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "Ozan05avci@gmail.com",
        "ascAppId": "6745790562",
        "appleTeamId": "9RFBD6KWF9",
        "language": "de-DE"
      }
    }
  }
}
